{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/public"
      }
    },
    {
      "src": "api/auth.ts",
      "use": "@vercel/node"
    },
    {
      "src": "api/dashboard.ts",
      "use": "@vercel/node"
    },
    {
      "src": "api/analytics.ts",
      "use": "@vercel/node"
    },
    {
      "src": "api/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/auth/login",
      "dest": "/api/auth"
    },
    {
      "src": "/api/dashboard/(.*)",
      "dest": "/api/dashboard"
    },
    {
      "src": "/api/analytics/(.*)",
      "dest": "/api/analytics"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/public/$1"
    }
  ]
}